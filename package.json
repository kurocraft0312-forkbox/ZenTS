{
  "name": "zents",
  "version": "0.1.3",
  "description": "ZenTS is a Node.js & TypeScript MVC-Framework for building rich web applications, released as free and open-source software under the MIT License. It is designed for building web applications with modern tools and design patterns.",
  "keywords": [
    "ZenTS",
    "framework",
    "http",
    "rest",
    "API",
    "MVC",
    "SOLID",
    "controller",
    "template",
    "service",
    "ORM"
  ],
  "author": "Sascha Habbes <sahachide@gmail.com>",
  "license": "MIT",
  "homepage": "http://zents.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahachide/ZenTS"
  },
  "bugs": {
    "url": "https://github.com/sahachide/ZenTS/issues"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc-watch --onSuccess \"npm run dev:update\"",
    "lint": "eslint . -f codeframe",
    "test": "jest --config jest.config.json",
    "release": "standard-version",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:typedoc": "typedoc --plugin typedoc-plugin-markdown --out docs/api src",
    "docs:deploy": "sh deploy-docs.sh",
    "dev:update": "yalc publish --push --changed",
    "dev:clean": "yalc installations clean zents"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md",
    "ATTRIBUTION.md"
  ],
  "dependencies": {
    "@types/formidable": "1.0.31",
    "@types/nunjucks": "3.1.3",
    "app-root-path": "3.0.0",
    "consola": "2.14.0",
    "cookie": "~0.4.1",
    "cosmiconfig": "~6.0.0",
    "find-my-way": "~3.0.1",
    "formidable": "2.0.0-canary.20200504.1",
    "lodash.merge": "~4.6.2",
    "mime-types": "~2.1.27",
    "nunjucks": "~3.2.2",
    "parseurl": "~1.3.3",
    "qs": "~6.9.4",
    "reflect-metadata": "0.1.13",
    "serve-static": "1.14.1",
    "statuses": "2.0.0",
    "type-fest": "0.16.0",
    "typeorm": "~0.2.25"
  },
  "devDependencies": {
    "@kawarimidoll/vuepress-plugin-tailwind": "^0.1.9",
    "@silvanite/vuepress-plugin-markdown-classes": "^1.0.2",
    "@types/app-root-path": "^1.2.4",
    "@types/cookie": "^0.4.0",
    "@types/cosmiconfig": "^6.0.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.158",
    "@types/lodash.merge": "^4.6.6",
    "@types/mime-types": "^2.1.0",
    "@types/node": "14.0.24",
    "@types/parseurl": "^1.3.1",
    "@types/qs": "^6.9.4",
    "@types/serve-static": "^1.13.5",
    "@types/statuses": "^2.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@vuepress/plugin-back-to-top": "^1.5.4",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "standard-version": "^9.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "tsc-watch": "^4.2.9",
    "typedoc": "^0.19.1",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "3.9.7",
    "vuepress": "^1.5.4",
    "vuepress-plugin-code-copy": "^1.0.6",
    "vuepress-plugin-element-tabs": "^0.2.8",
    "vuepress-plugin-minimal-analytics": "^0.1.4",
    "vuepress-plugin-reading-progress": "^1.0.10",
    "vuepress-plugin-reading-time": "^0.1.1",
    "vuepress-theme-default-prefers-color-scheme": "^1.1.0",
